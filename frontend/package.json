{
  "name": "kindred-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:default": "vite",
    "build": "tsc && vite build && node scripts/fix-css-infinity.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "type-check": "tsc --noEmit && tsc --noEmit -p tsconfig.node.json",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --reporter=tree --coverage",
    "generate-types": "node scripts/generate-types.js"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.12.1",
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.11",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.9",
    "@popperjs/core": "^2.11.8",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/query-sync-storage-persister": "^5.90.21",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-persist-client": "^5.90.21",
    "@tanstack/react-virtual": "^3.13.18",
    "@vitejs/plugin-react": "^5.1.2",
    "clsx": "^2.1.1",
    "codemirror": "^6.0.2",
    "cytoscape": "^3.32.0",
    "cytoscape-bubblesets": "^4.0.0",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-fcose": "^2.2.0",
    "cytoscape-popper": "^4.0.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "pocketbase": "^0.26.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.4.1",
    "react-router": "^7.13.0",
    "recharts": "^3.6.0",
    "tailwindcss": "^4.1.11",
    "tippy.js": "^6.3.7",
    "zod": "^4.3.5"
  },
  "overrides": {
    "@tanstack/query-core": "5.90.13"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.91.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^27.4.0",
    "pocketbase-typegen": "^1.3.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "npm:rolldown-vite@^7.3.0",
    "vitest": "^4.0.16"
  }
